












// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int n;
    cin>>n;
    for(int i = 1 ; i<=n ; i++){
        for(int j = n-1 ; j>=i ; j--){
            cout<<" ";
        }
            for(int k = 1 ; k<=i ; k++){
                cout<<k;
                
                
            }
        
                         cout<<endl;
        } 
    
       
    }
