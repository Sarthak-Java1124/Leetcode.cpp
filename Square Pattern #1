{
    int n;
    cin>>n;
    int k = (n+1)/2;
   for(int i = 1 ; i<=n ; i++){
       for(int j = 1 ; j<=n ; j++){
           if(i==1 || j==1 || i==n || j==n){
               cout<<"*";
           }else{
               cout<<" ";
           }

       }
              cout<<endl;
   }
        
}

output :
n=5 
5
